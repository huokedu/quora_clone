<div class="newSessionWrapper">
  <div class="background-image"></div>
  <div class="header-container">
    <h1 class="logo"> Quorum </h1>
    <h3 class="tagline"> The best answer to any question. </h3>
  </div>

    <h1>YOU ARE IN THE OUT-MODED NEW SESSION VIEW</h1>

  <%= render "shared/errors" %>

  <form action="<%= session_url %>" method="post">
    <input
      type="hidden"
      name="authenticity_token"
      value="<%= form_authenticity_token %>">

    <label for="user_username">User Name</label>
    <input type="text" name="user[user_name]" id="user_username">
    <br>

    <label for="user_password">Password</label>
    <input type="password" name="user[password]" id="user_password">
    <br>

    <input type="submit">
  </form>
</div>
